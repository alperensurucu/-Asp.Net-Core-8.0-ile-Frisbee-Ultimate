 @model Course

<<div class="card mb-3" id="@Model.Id">
    <div class="row">
        <div class="col-md-4">
            <img src="~/img/@Model.image" class="img-fluid rounded-start" alt="" />
        </div>
        <div class="col-md-8">
            <h5 class="card-title">@Model.Title"ye hoşgeldiniz"</h5>
            <p class="card-text">@Model.Description</p>
        </div>
    </div>
</div>